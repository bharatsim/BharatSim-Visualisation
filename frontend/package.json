{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@date-io/date-fns": "1.x",
    "@material-ui/core": "4.11.4",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/pickers": "^3.3.10",
    "axios": "0.21.1",
    "changedpi": "^1.0.4",
    "chroma-js": "^2.1.0",
    "clsx": "^1.1.1",
    "date-fns": "^2.18.0",
    "draft-js": "^0.11.7",
    "fast-deep-equal": "^3.1.3",
    "final-form": "^4.20.2",
    "final-form-arrays": "^3.0.2",
    "html-to-image": "^1.3.21",
    "http-proxy-middleware": "2.0.0",
    "immer": "^9.0.3",
    "leaflet": "^1.7.1",
    "leaflet.heat": "^0.2.0",
    "lodash.get": "^4.4.2",
    "material-table": "^1.69.1",
    "mathjs": "^9.3.2",
    "notistack": "^1.0.1",
    "papaparse": "^5.2.0",
    "plotly.js": "^2.0.0",
    "prismjs": "^1.23.0",
    "prop-types": "^15.7.2",
    "react": "17.0.2",
    "react-color": "^2.19.3",
    "react-dom": "17.0.2",
    "react-draft-wysiwyg": "^1.14.7",
    "react-error-boundary": "^3.1.1",
    "react-final-form": "^6.5.2",
    "react-final-form-arrays": "^3.1.3",
    "react-grid-layout": "1.2.5",
    "react-json-view": "^1.19.1",
    "react-leaflet": "^3.0.5",
    "react-plotly.js": "^2.5.1",
    "react-redux": "^7.2.2",
    "react-resizable": "3.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-simple-code-editor": "^0.11.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false --coverage --env=jest-environment-jsdom-sixteen --resetMocks=false",
    "eject": "react-scripts eject",
    "lint": "eslint src",
    "format": "prettier src",
    "fix": "eslint src --fix && prettier src --write "
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "*.{js,jsx,css,html,md,json,test.jsx,test.js}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "globalSetup": "./src/globalSetup.js",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/constants/*.js",
      "!src/setupProxy.js",
      "!src/setupTests.js",
      "!src/config/*",
      "!src/index.js",
      "!src/**/*.labels.js",
      "!src/**/colorPalette.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 99,
        "functions": 99.18,
        "lines": 99.5,
        "statements": 99.5
      }
    },
    "coverageReporters": [
      "text",
      "json",
      "lcov"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.2",
    "@testing-library/dom": "7.31.2",
    "@testing-library/jest-dom": "5.13.0",
    "@testing-library/react": "11.2.7",
    "@testing-library/react-hooks": "7.0.0",
    "@testing-library/user-event": "12.0.11",
    "babel-eslint": "10.x",
    "eslint": "7.28.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "5.7.2",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "4.2.0",
    "history": "^5.0.0",
    "jest-canvas-mock": "2.3.1",
    "jest-environment-jsdom-sixteen": "^2.0.0",
    "lint-staged": "11.0.0",
    "prettier": "2.3.1",
    "react-test-renderer": "17.0.2",
    "redux-saga-testing": "^2.0.1"
  }
}
